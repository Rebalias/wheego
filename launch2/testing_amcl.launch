<?xml version="1.0"?>

<!--Edited from hybrid_astar/manual.launch-->

<launch>
  

  <node name="map_server" pkg="map_server" type="map_server" 
    args="$(find wheego)/maps/testmap3.yaml">
    <remap from="map" to="map"/>
  </node>



  <param name="/use_sim_time" value="true"/>

  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find wheego)/launch/mapNav.rviz" >
   <param name="/use_sim_time" value="true"/>
  </node>

 <include file="$(find wheego)/launch/pc2scan.launch"/>

  <node pkg="tf" type="static_transform_publisher"
    name="base_to_laser_broadcaster"
    args="0 0 0 0 0 0 base_link velodyne 100"/>

  <node pkg="tf" type="static_transform_publisher"
    name="h_map"
    args="0 0 0 0 0 0 map mapcurrent 100"/>


  <include file="$(find wheego)/launch/odomtransformer.launch">
  </include>

  <node pkg="tf" type="static_transform_publisher"
    name="map_nav_map_bcast"
    args="0 0 0 0 0 0 map base_footprint 100"/>

  <node pkg="tf" type="static_transform_publisher"
    name="map_to_odom_broadcaster"
    args="0 0 0 0 0 0 map scanmatch_odom 100"/>

  <include file="$(find wheego)/launch/amcl_hector.launch">
  </include>





  <include file="$(find wheego)/launch/amcl2.launch"/>


</launch>
